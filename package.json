{
	"name": "@ctx-core/fontawesome-ui-svelte",
	"version": "1.0.10",
	"description": "ctx-core fontawesome",
	"keywords": [
		"ctx-core",
		"fontawesome"
	],
	"homepage": "https://github.com/ctx-core/fontawesome-ui-svelte#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/fontawesome-ui-svelte/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fontawesome-ui-svelte.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"bin": {
		"refresh-font-awesome": "./bin/refresh-font-awesome.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"type": "module",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.40",
		"@ctx-core/svg": "workspace:^16.0.7",
		"domutils": "^2.8.0",
		"globby": "^12.0.2",
		"htmlparser2": "^7.2.0",
		"import-meta-resolve": "^1.1.1"
	},
	"devDependencies": {
		"@ctx-core/cli-args": "workspace:^11.0.18",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"rimraf": "^3.0.2",
		"svelte": "^3.44.3",
		"svelte-preprocess": "^4.10.0",
		"svelte2tsx": "^0.4.11",
		"tslib": "^2.3.1",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}
