{
	"name": "@ctx-core/fontawesome-ui-svelte",
	"version": "1.1.159",
	"description": "ctx-core fontawesome",
	"keywords": [
		"ctx-core",
		"fontawesome"
	],
	"homepage": "https://github.com/ctx-core/fontawesome-ui-svelte#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/fontawesome-ui-svelte/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fontawesome-ui-svelte.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"bin": {
		"refresh-font-awesome": "./bin/refresh-font-awesome.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "uvu . '\\.test\\.(ts|tsx|js|jsx)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./bin",
		"./ui"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"dependencies": {
		"@ctx-core/svg": "^19.0.72",
		"@ctx-core/svg-ui-svelte": "^1.3.158",
		"ctx-core": "^6.2.3",
		"dom-serializer": "^2.0.0",
		"domutils": "^3.1.0",
		"htmlparser2": "^9.1.0",
		"import-meta-resolve": "^4.0.0",
		"svelte": "^4.2.10",
		"tiny-glob": "^0.2.9"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.1",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}